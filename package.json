{
  "name": "morelogin-cli",
  "version": "1.0.2",
  "description": "Morelogin browser automation CLI tool - Local API only",
  "main": "bin/morelogin.js",
  "bin": {
    "morelogin": "./bin/morelogin.js"
  },
  "scripts": {
    "start": "node bin/morelogin.js",
    "test:validate": "node scripts/cli-validation-smoke.js",
    "test:e2e": "node scripts/cli-real-e2e.js",
    "test:e2e:strict": "node scripts/cli-real-e2e.js --strict --start-attempts=3 --start-timeout-ms=30000",
    "example:basic": "node examples/basic-example.js",
    "example:form": "node examples/form-example.js",
    "example:scraping": "node examples/scraping-example.js",
    "example:multi-tab": "node examples/multi-tab-example.js",
    "example:puppeteer": "node examples/puppeteer-example.js",
    "example:playwright": "node examples/playwright-example.js"
  },
  "keywords": [
    "morelogin",
    "browser",
    "automation",
    "cdp",
    "local-api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "playwright": "^1.40.0",
    "puppeteer-core": "^21.11.0"
  }
}
